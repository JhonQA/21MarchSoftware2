package com.qa.results;

public class FlowChartRecreation {
	
	public static int recreation(int x){
		
	if(x>2000) {
		System.out.println("A");
	}		if(x>6000) {
			System.out.println("C");
		}else {
			if(x<6000) {
				System.out.println("B");
			}else if(x<4000) {
				System.out.println("D");
			}else {
				if(x>4000) {
					System.out.println("E");
				}
			}
		}
	
	
	
		return x;

}
}
